<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.konstantin.playergamekm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.TicTacToeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.GameEmulatorActivity" />
        <activity android:name=".Activities.ViewScoreboardActivity" />
        <activity android:name=".Activities.SelectPlayerActivity" />
        <activity android:name=".Activities.DeletePlayerActivity" />
        <activity android:name=".Activities.AboutActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.SelectTicTacToePlayerActivity"></activity>
    </application>

</manifest>